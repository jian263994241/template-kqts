{
  "arrowParens": "always",
  "singleQuote": true,
  "parser": "babylon",
  "semi": true,
  "tabWidth": 2,
  "bracketSameLine": false,
  "bracketSpacing": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    },
    {
      "files": ".babelrc",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser": "scss",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.less",
      "options": {
        "parser": "less",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "html",
        "trailingComma": "none"
      }
    }
  ]
}
